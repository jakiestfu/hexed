{
  "name": "@hexed/workbench",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "build": "tsc -p tsconfig.declarations.json",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,mdx}\" --ignore-path=\"../.prettierignore\"",
    "format:fix": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,mdx}\" --ignore-path=\"../../.prettierignore\""
  },
  "dependencies": {
    "@hexed/file": "workspace:*",
    "@hexed/plugins": "workspace:*",
    "@hexed/ui": "workspace:*",
    "@hexed/worker": "workspace:*",
    "@monaco-editor/react": "^4.7.0",
    "constrained-editor-plugin": "^1.4.0",
    "lucide-react": "^0.468.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@hexed/eslint-config": "workspace:*",
    "@hexed/prettier-config": "workspace:*",
    "@hexed/typescript-config": "workspace:*",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "peerDependencies": {
    "monaco-editor": "^0.55.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "prettier": "@hexed/prettier-config"
}