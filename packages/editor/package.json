{
  "name": "@hexed/editor",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,mdx}\" --ignore-path=\"../.prettierignore\"",
    "format:fix": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,mdx}\" --ignore-path=\"../../.prettierignore\""
  },
  "dependencies": {
    "@hexed/templates": "workspace:*",
    "@hexed/file": "workspace:*",
    "@hexed/canvas-react": "workspace:*",
    "@hexed/logger": "workspace:*",
    "@hexed/types": "workspace:*",
    "@hexed/ui": "workspace:*",
    "@hexed/worker": "workspace:*",
    "lucide-react": "^0.468.0",
    "react-hotkeys-hook": "^4.5.1",
    "react-markdown": "^9.0.1",
    "react-window": "^1.8.10",
    "recharts": "2.15.4"
  },
  "devDependencies": {
    "@hexed/eslint-config": "workspace:*",
    "@hexed/prettier-config": "workspace:*",
    "@hexed/typescript-config": "workspace:*",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^1.8.8",
    "@types/wicg-file-system-access": "^2023.10.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.7.2"
  },
  "prettier": "@hexed/prettier-config",
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}