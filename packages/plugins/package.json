    "@hexed/templates": "workspace:*",
    "@hexed/ui": "workspace:*",
    "@hexed/worker": "workspace:*",
    "chart.js": "^4.4.0",
    "lucide-react": "^0.468.0",
    "react-hotkeys-hook": "^4.5.1",
    "react-window": "^1.8.10"
  },
  "devDependencies": {
    "@hexed/eslint-config": "workspace:*",
    "@hexed/prettier-config": "workspace:*",
    "@hexed/typescript-config": "workspace:*",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^1.8.8",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.7.2"
  },
  "prettier": "@hexed/prettier-config",
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}