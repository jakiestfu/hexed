    "@hexed/eslint-config": "workspace:*",
    "@hexed/prettier-config": "workspace:*",
    "@hexed/typescript-config": "workspace:*",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "prettier": "@hexed/prettier-config"
