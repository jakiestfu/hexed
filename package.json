{
  "name": "hexed",
  "version": "0.3.0",
  "private": true,
  "description": "A modern hex editor for inspecting and tracking binary file changes",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakiestfu/hexed.git"
  },
  "homepage": "https://jakiestfu.github.io/hexed",
  "scripts": {
    "puppeteer:install": "pnpm puppeteer browsers install chrome",
    "screenshot": "node ./scripts/screenshots.mjs",
    "create-public-package": "node scripts/create-public-package.mjs",
    "predev": "pnpm create-public-package",
    "prebuild": "pnpm create-public-package",
    "postinstall": "pnpm create-public-package",
    "dev": "pnpm --filter @hexed/web dev",
    "preview": "pnpm --filter @hexed/web preview",
    "build": "pnpm -r build",
    "dev:desktop": "pnpm --filter desktop dev",
    "build:desktop": "pnpm --filter desktop build",
    "release": "node scripts/deploy.mjs",
    "postrelease": "command -v afplay >/dev/null 2>&1 && afplay /System/Library/Sounds/Submarine.aiff || true",
    "_typecheck": "pnpm -r typecheck",
    "clean": "rm -rf node_modules",
    "clean:workspace": "pnpm -r clean && pnpm clean",
    "format": "pnpm -r format",
    "format:fix": "pnpm -r format:fix",
    "lint": "pnpm -r lint && manypkg check",
    "lint:fix": "pnpm -r lint:fix && manypkg fix",
    "typecheck": "pnpm -r typecheck",
    "analyze": "pnpm -r analyze",
    "version:minor": "pnpm version minor --no-git-tag-version && pnpm create-public-package",
    "version:patch": "pnpm version patch --no-git-tag-version && pnpm create-public-package"
  },
  "keywords": [
    "hex-editor",
    "binary",
    "inspector"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=25",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.25.1",
    "gh-pages": "^6.1.1",
    "http-server": "^14.1.1",
    "puppeteer": "^24.36.0",
    "typescript": "^5.7.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ],
    "overrides": {
      "next": "15.5.9",
      "react": "19.2.3",
      "react-dom": "19.2.3"
    }
  }
}
