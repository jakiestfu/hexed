{
  "name": "hexed",
  "version": "1.0.0",
  "private": true,
  "description": "A hex editor for inspecting and tracking binary file changes",
  "scripts": {
    "dev": "pnpm --filter @hexed/web dev",
    "build": "pnpm --filter @hexed/web build",
    "start": "pnpm --filter @hexed/web start",
    "dev:desktop": "pnpm --filter desktop dev",
    "build:desktop": "pnpm --filter desktop build",
    "_typecheck": "pnpm -r typecheck",
    "clean": "rm -rf node_modules",
    "clean:workspace": "pnpm -r clean && pnpm clean",
    "format": "pnpm -r format",
    "format:fix": "pnpm -r format:fix",
    "lint": "pnpm -r lint && manypkg check",
    "lint:fix": "pnpm -r lint:fix && manypkg fix",
    "typecheck": "pnpm -r typecheck",
    "analyze": "pnpm -r analyze"
  },
  "keywords": [
    "hex-editor",
    "binary",
    "inspector"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=25",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "turbo": "^2.7.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ],
    "overrides": {
      "next": "15.5.9",
      "react": "19.2.3",
      "react-dom": "19.2.3"
    }
  }
}
